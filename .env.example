# サーバー設定
PORT=3000                          # アプリケーションのポート番号
HOST=localhost                     # アプリケーションのホスト名

# 外部株価API設定
STOCK_API_KEY=your_api_key_here    # 外部株価APIの認証キー
STOCK_API_URL=https://api.example.com/stocks  # 外部株価APIのベースURL
API_TIMEOUT=5000                   # API呼び出しのタイムアウト時間(ミリ秒)

# ログ設定
LOG_LEVEL=info                     # ログレベル(debug, info, warn, error)

# GitHub Packages認証
NPM_TOKEN=your_github_token_here   # GitHub Personal Access Token 